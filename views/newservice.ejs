<%- include('./partials/header') %>
    <link rel="stylesheet" type="text/css" href="/stylesheets/registerStyle.css">
    <div class="container" id="mainContainer">
        <h1 class="well" id="formHead">Add new buisness and services</h1>

        <form action="/service" method="POST">
            <div class="row">
                <div class="col-sm-6 form-group">
                    <label>Buisness/Services Name</label>
                    <input type="text" name="servicename" placeholder="Enter the service Name here.." class="form-control">
                </div>
                <div class="col-sm-6 form-group">
                    <label>Buisness/Service Type</label>
                    <input type="text" name="servicetype" placeholder="Enter the producer type here.." class="form-control">
                </div>
            </div>
            <div class="form-group">
                <label>Owner</label>
                <input type="text" name="owner" placeholder="Enter service name/company/Gruha Udyog name here.." rows="3" class="form-control">
            </div>
            <div class="row">
                <div class="col-sm-4 form-group">
                    <label>City</label>
                    <input type="text" name="city" placeholder="Your production city.." class="form-control">
                </div>
                <div class="col-sm-4 form-group">
                    <label>State</label>
                    <input type="text" name="state" placeholder="Your production State.." class="form-control">
                </div>
            </div>
            <div class="form-group">
                <label>Phone Number</label>
                <input type="text" name="phone" placeholder="Your  Phone Number here.." class="form-control">
            </div>

            <div class="form-group">
                <label>Sample Image URL</label>
                <input type="text" name="image" placeholder="" class="form-control">
            </div>

            <div class="form-group">
                <label>Email Address</label>
                <input type="text" name="email" placeholder="Enter price here.." class="form-control">
            </div>

            <div class="form-group">
                <label>Short Description of what u produce.</label>
                <input type="text" name="description" placeholder="" class="form-control">
            </div>
            <button class="btn btn-lg btn-info">Submit</button>
        </form>
    </div>

    <%- include('./partials/footer') %>